{
  "name": "svelte-vgs",
  "repository": {
    "type": "git",
    "url": "https://github.com/thorcc/svelte-vgs.git"
  },
  "displayName": "svelte-vgs",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.63.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:svelte-vgs.createSvelte",
    "onCommand:svelte-vgs.buildSvelte",
    "onCommand:svelte-vgs.runSvelte"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "svelte-vgs.createSvelte",
        "title": "Create new Svelte project"
      },
      {
        "command": "svelte-vgs.buildSvelte",
        "title": "Build Svelte project"
      },
      {
        "command": "svelte-vgs.runSvelte",
        "title": "Run Svelte project"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "svelte-vgs.createSvelte",
          "group": "SvelteGroup@1"
        },
        {
          "command": "svelte-vgs.buildSvelte",
          "group": "SvelteGroup@1"
        },
        {
          "command": "svelte-vgs.runSvelte",
          "group": "SvelteGroup@1"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.63.0",
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "eslint": "^8.1.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.3",
    "typescript": "^4.4.4",
    "@vscode/test-electron": "^1.6.2"
  }
}
